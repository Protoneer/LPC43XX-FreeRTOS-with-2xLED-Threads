// BK
#include "FreeRTOS.h"
#include "task.h"
#include "timers.h"
#include "semphr.h"


#include "include/LED.H"

__CRP const unsigned int CRP_WORD = CRP_NO_CRP ;

uint32_t msec;
uint32_t tempx, tempy;


int BK_TEST(void)
{
	// Needed
	SystemInit();
	CGU_Init();

	SetupLEDS();

	while (1)
	{                           					// Loop forever
		LED1();
		LED2();
	}
	return 0;
}



int main(void)
{
	vTaskStartScheduler();

	return BK_TEST();
}

